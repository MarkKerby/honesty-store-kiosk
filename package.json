{
	"name": "honesty-store-kiosk",
	"description": "Kiosk client for honesty store in scottLogic NCL office",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-scripts": "1.1.4",
		"react-webcam": "^1.0.1"
	},
	"scripts": {
		"prettify": "prettier --write ./src/**/*.js",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject"
	},
	"husky": {
		"hooks": {
			"precommit --no-verify": ["npm run prettify"]
		}
	},
	"devDependencies": {
		"husky": "^0.14.3",
		"lint-staged": "^7.2.0",
		"prettier": "1.13.7"
	},
	"eslintConfig": {
		"parserOptions": {
			"ecmaVersion": 6,
			"sourceType": "module",
			"ecmaFeatures": {
				"jsx": true
			}
		}
	}
}
